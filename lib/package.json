{
  "name": "msal-vue",
  "type": "module",
  "version": "1.0.0",
  "description": "Microsoft Authentication Library for Vue",
  "author": {
    "name": "Jove Jonovski",
    "email": "jovejonovski@gmail.com",
    "url": "https://github.com/datyin"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "msal",
    "azure",
    "@azure/msal",
    "@azure/msal-browser",
    "auth",
    "auth-browser"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "ts:check": "vue-tsc && vite build",
    "ts:dts": "vue-tsc -p tsconfig.build.json",
    "dev": "vite",
    "build": "pnpm run lint && pnpm run ts:check && vite build && pnpm run ts:dts"
  },
  "peerDependencies": {
    "@azure/msal-browser": "^3.14.0",
    "vue": "^3.3.0"
  }
}
